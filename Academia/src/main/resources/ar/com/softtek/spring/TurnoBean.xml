<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 
 	<bean id="turnoAction" class="ar.com.softtek.action.TurnoAction">
		<property name="turnoService" ref="turnoService" />	
	</bean>
	
	<bean id="turnoService" class="ar.com.softtek.service.impl.TurnoServiceImpl">
		<property name="turnoBo" ref="turnoBo" />	
	</bean>
	
	<bean id="turnoBo" class="ar.com.softtek.bo.impl.TurnoBoImpl" >
		<property name="turnoDAO" ref="turnoDAO" />
	</bean>
	
   	<bean id="turnoDAO" class="ar.com.softtek.dao.impl.TurnoDAOImpl" >
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
 
</beans>