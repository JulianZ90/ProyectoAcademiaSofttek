<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 
 	<bean id="cambioPlanAction" class="ar.com.softtek.action.CambioPlanAction">
		<property name="cambioPlanService" ref="cambioPlanService" />	
	</bean>
	
	<bean id="cambioPlanService" class="ar.com.softtek.service.impl.CambioPlanImpl">
		<property name="cambioPlanBo" ref="cambioPlanBo" />	
	</bean>

	<bean id="cambioPlanBo" class="ar.com.softtek.bo.impl.CambioPlanBoImpl" >
		<property name="cambioPlanDAO" ref="cambioPlanDAO" />
	</bean>
	
   	<bean id="cambioPlanDAO" class="ar.com.softtek.dao.impl.CambioPlanDAOImpl" >
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
 
</beans>