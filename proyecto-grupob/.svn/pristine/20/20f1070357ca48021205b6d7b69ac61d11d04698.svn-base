<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
 	<bean id="turnoController" class="ar.com.softtek.grupob.controller.TurnoController">
		<property name="turnoService" ref="turnoService" />	
	</bean>
 	
 	
 	<bean id="turnoService" class="ar.com.softtek.grupob.bo.impl.TurnoServiceImpl" >
		<property name="turnoDAO" ref="turnoDAO" />
	</bean>
	
   	<bean id="turnoDAO" class="ar.com.softtek.grupob.dao.impl.TurnoDAOImpl" >
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
</beans>