<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ar.com.softtek.grupob.entity.Prestador" 
		table="Prestadores" >
        <id name="idPrestador" type="int">
            <column name="id_Prestador" />
        </id>
        <property name="apellidoNombre" type="string">
            <column name="ApellidoYNombre" length="250"/>
        </property>
        <property name="filial" type="string">
            <column name="Filial" length="250"/>
        </property>
        <property name="especialidad" type="string">
            <column name="Especialidad" length="250"/>
        </property>
        <property name="domicilio" type="string">
            <column name="Domicilio" length="250"/>
        </property>
        <property name="localidad" type="string">
            <column name="Localidad" length="250"/>
        </property>
        <property name="telefono" type="string">
            <column name="Telefono" length="250"/>
        </property>
    </class>
</hibernate-mapping>