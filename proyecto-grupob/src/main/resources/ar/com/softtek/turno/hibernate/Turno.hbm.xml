<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ar.com.softtek.model.Turno"  table="Turnos" >
        <id name="idTurno" column="idTurno" type="java.lang.Integer">
            <generator class="identity" />
        </id>
        <property name="nombre" column="nombre" type="java.lang.String"/>
        <property name="apellido" column="apellido" type="java.lang.String"/>
        <property name="idPaciente" column="idPaciente" type="java.lang.Integer"/>
	<property name="planMedico" column="planMedico" type="java.lang.Integer"/>
        <property name="fechaTurno" column="fechaTurno" type="java.lang.String"/>
        <property name="horaTurno" column="horaTurno" type="java.lang.String"/>
        <property name="fechaLlegada" column="fechaLlegada" type="java.lang.String"/>
        <property name="horaLlegada" column="horaLlegada" type="java.lang.String"/>
        <property name="fechaAtencion" column="fechaAtencion" type="java.lang.String"/>
        <property name="fechaAtencion" column="fechaAtencion" type="java.lang.String"/>
        <property name="idPrestador" column="idPrestador" type="java.lang.Integer"/>
        <property name="idPractica" column="idPractica" type="java.lang.Integer"/>
        <property name="importe" column="importe" type="java.lang.Integer"/>
        <property name="observaciones" column="observaciones" type="java.lang.String"/>
    </class>
</hibernate-mapping>
